<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Brushes -->
  <RadialGradientBrush x:Key="PrimaryGradient"
                       Center="0.5,0.5"
                       GradientOrigin="0.5,0.5"
                       Radius="0.8">
    <GradientStop Color="#FFF9FBFD" Offset="0"/>
    <GradientStop Color="#FFEFF3F7" Offset="1"/>
  </RadialGradientBrush>

  <LinearGradientBrush x:Key="SecondaryGradient" StartPoint="0,0" EndPoint="1,1">
    <GradientStop Color="#FFEAECF5" Offset="0"/>
    <GradientStop Color="#FFD5D9E6" Offset="1"/>
  </LinearGradientBrush>

  <LinearGradientBrush x:Key="AccentGradient" StartPoint="0,0" EndPoint="1,1">
    <GradientStop Color="#FF7A8CF2" Offset="0"/>
    <GradientStop Color="#FF5061BD" Offset="1"/>
  </LinearGradientBrush>




</ResourceDictionary>
